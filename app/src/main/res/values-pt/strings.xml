    <!--OPERATORS UNIQUE GADGET DESCRIPTION-->
    <string name="ugat_ash">Munição alojadora M120</string>
    <string name="uga_ash">Munição Explosiva que se prende a uma superfície e detona automaticamente.</string>

    <string name="ugat_bandit">Eletrocutador CED-1</string>
    <string name="uga_bandit">Coloca Fios de Choque de alta tensão em gadgets metálicos para causar dano extra ao inimigo.</string>

    <string name="ugat_blackbeard">Escudo TARS MK 0</string>
    <string name="uga_blackbeard">Tem um escudo reserva acoplado em caso de o prirneiro ser danificadu.</string>

    <string name="ugat_blitz">Escudo tático G52</string>
    <string name="uga_blitz">Aciona o feixe de luz acoplado ao escudo balístico para cegar temporariamente os inimigos.</string>

    <string name="ugat_buck">SK 4–12</string>
    <string name="uga_buck">Ativa um tambor de espingarda como adicional na sua arma principal.</string>

    <string name="ugat_capitao">TAC Mk0</string>
    <string name="uga_capitao">Balestra com munição asfixiante e micro granadas de fumaça.</string>

    <string name="ugat_castle">Painel protetor universal PPU1</string>
    <string name="uga_castle">Monta barricadas à prova de balas, mais resistente que as normais.</string>

    <string name="ugat_caveira">Passos silenciosos</string>
    <string name="uga_caveira">Caveira possui as habilidades furtivas dos "Passos silenciosos", que a tornam uma caçadora formidável. Rápida e silenciosa.</string>

    <string name="ugat_doc">Pistola de dose estimulante MPD-O</string>
    <string name="uga_doc">Atira uma dose subcutânea para reanimar a si próprio ou a um colega de equipe. Também oferece reforço temporário.</string>

    <string name="ugat_echo">YOKAI</string>
    <string name="uga_echo">Drone flutuante capaz de lançar ondas acústicas atordoantes. O Yokai também transmite sinal de vídeo para o Agente.</string>

    <string name="ugat_frost">Sterling MK2 LHT</string>
    <string name="uga_frost">Utiliza uma armadilha mecânica para incapacitar inimigos.</string>

    <string name="ugat_fuze">Perfurador APM-6 "Matryoshka"</string>
    <string name="uga_fuze">Utiliza Dispositivos Perfuradores que se prendem à parede, lançando granadas para o lado oposto.</string>

    <string name="ugat_glaz">Mira de precisão HDS-5x</string>
    <string name="uga_glaz">Ativa uma lente de aumento com sensores avançados que delineam alvos a longa distância, mesmo através de ambientes esfumaçados.</string>

    <string name="ugat_hibana">X-KAIROS</string>
    <string name="uga_hibana">Um lançador de granadas compatível com munições de 40 mm. Os projétais explosivos podem ser detonados a distância.</string>

    <string name="ugat_iq">Detector RED MK III "Spectre"</string>
    <string name="uga_iq">Utiliza um detector para localizar equipamentos eletrônicos a curta distância através de paredes.</string>

    <string name="ugat_jackal">EYENOX MODEL III</string>
    <string name="uga_jackal">Item de rastreamento por identificação de pegadas recentes.</string>

    <string name="ugat_jager">Desativador ADS-MKIV "Magpie"</string>
    <string name="uga_jager">Utiliza um sistema de Defesa Ativa que intercepta granadas antes da detonação.</string>

    <string name="ugat_kapkan">Sensor explosivo EDD MK II</string>
    <string name="uga_kapkan">Coloca armadilhas em janelas e soleiras de portas.</string>

    <string name="ugat_mira">ESPELHO NEGRO</string>
    <string name="uga_mira">Instala um espelho protetor blindado em paredes quebráveis ou reforçadas. Pode ser retirado para uma abertura estratégica.</string>

    <string name="ugat_montagne">Le Roc</string>
    <string name="uga_montagne">Pode ajustar seu Escudo Extensível para oferecer proteção completa quando estiver em pé.</string>

    <string name="ugat_mute">Disruptor de sinal GC90 "Moni"</string>
    <string name="uga_mute">Afeta o sinal de comunicação de gadgets ou drones controlados remotamente.</string>

    <string name="ugat_pulse">Sensor cardíaco HB-5</string>
    <string name="uga_pulse">Detecta batimentos cardíacos a curta distância através de obstáculos.</string>

    <string name="ugat_rook">Blindagem "Rhino" R1N</string>
    <string name="uga_rook">Solta uma bagagem cheia de suprimentos de blindagem para uso da sua equipe.</string>

    <string name="ugat_sledge">Marreta de cabo longo</string>
    <string name="uga_sledge">Utiliza uma Marreta para destruir superfícies demolíveis.</string>

    <string name="ugat_smoke">Granada de gás composto Z8</string>
    <string name="uga_smoke">Libera cargas de gás tóxico detonadas remotamente.</string>

    <string name="ugat_tachanka">Metralhadora RP-46 Degtyaryov</string>
    <string name="uga_tachanka">Instala uma Metralhadora Fixa com um escudo protegendo a cabeça contra disparos frontais.</string>

    <string name="ugat_thatcher">Granada eletromagnética EG MKO</string>
    <string name="uga_thatcher">Utiliza Granadas EMP para desativar equipamentos eletrônicos nas proximidades.</string>

    <string name="ugat_thermite">Carga exotérmica BRIMSTONE BC-3</string>
    <string name="uga_thermite">Libera uma carga exotérmica que destrói paredes reforçadas.</string>

    <string name="ugat_twitch">Drone de choque RSD1</string>
    <string name="uga_twitch">Utiliza dois Drones de Choque para desativar armadilhas ou ferir inimigos. Utiliza 0 primeiro Drone durante a fase de preparação.</string>

    <string name="ugat_valkyrie">Câmera portátil Mk2</string>
    <string name="uga_valkyrie">Utiliza a câmera de ação "Black Eye", cujas imagens são acessíveis a toda a equipe com as ferramentas de observação.</string>

    <string name="ugat_ying">Candela</string>
    <string name="uga_ying">Combinação de explosivos que pode ser fixada a estruturas ou lançada como uma granada.</string>

    <string name="ugat_lesion">Gu</string>
    <string name="uga_lesion">Minas tóxicas camufladas que ferem o oponente e reduzem sua velocidade.</string>

    <string name="ugat_ela">Mina Grzmot</string>
    <string name="uga_ela">Minas de concussão aplicáveis a diversas superfícies, causando surdez e tontura.</string>

    <string name="ugat_zofia">KS79 LIFELINE</string>
    <string name="uga_zofia">Lançador de tambor duplo capaz de atirar granadas de concussão e de impacto.</string>

    <string name="ugat_dokkaebi">BOMBA LÓGICA</string>
    <string name="uga_dokkaebi">Carrega um vírus que faz com que os celulares dos inimigos emitam um som, o que pode comprometer a posição deles.</string>

    <string name="ugat_vigil">ERC-7</string>
    <string name="uga_vigil">Vigil escaneia dispositivos ao seu redor para apagar sua própria imagem.</string>

    <string name="ugat_lion">EE-ONE-D</string>
    <string name="uga_lion">O EE-ONE-D detecta movimento inimigo, proporcionando uma visão geral do campo batalha.</string>

    <string name="ugat_finka">PICO DE ADRENALINA</string>
    <string name="uga_finka">Os nanorrobôs dão um breve genho de energia à equipe e reanimam temporariamente os aliados feridos em estado crítico (DBNO).</string>

    <string name="ugat_maverick">Breaching Torch</string>
    <string name="uga_maverick">Blowtorch capable of burning holes reinforced walls at an almost inaudible level.</string>

    <string name="ugat_clash">CCE Shield</string>
    <string name="uga_clash">Extendable electro shield capable of slowing down enemies, causing damage over time.</string>

